{
  "name": "pdf-to-txt",
  "description": "A PDF to TXT converter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "lint": "next lint",
    "prepare": "husky",
    "prettier": "prettier . --write",
    "test": "vitest",
    "cy:open": "cypress open",
    "e2e": "cypress run",
    "local-build-and-serve": "NODE_ENV=test next build && serve out",
    "demo-mobile-video": "cypress run --spec cypress/e2e/upload-pdf-files-demo-video.cy.ts --no-runner-ui --config-file cypress-demo-mobile.config.ts",
    "demo-desktop-video": "cypress run --spec cypress/e2e/upload-pdf-files-demo-video.cy.ts --no-runner-ui --config-file cypress-demo-desktop.config.ts",
    "gif": "gifify -r 10@1 -o public/gifs/demo_mobile_video public/videos/demo_mobile_video.mp4"
  },
  "dependencies": {
    "@heroui/react": "^2.7.8",
    "framer-motion": "^12.10.5",
    "jspdf": "^3.0.1",
    "next": "^15.3.2",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-pdftotext": "^1.3.4",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "cypress": "^14.3.3",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@10.10.0"
}
