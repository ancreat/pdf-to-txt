{
  "name": "pdf-to-txt",
  "description": "A PDF to TXT converter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "lint": "next lint",
    "prepare": "husky",
    "prettier": "prettier . --write",
    "test": "vitest",
    "cy:open": "cypress open",
    "e2e": "cypress run",
    "demo-mobile-video": "cypress run --spec cypress/e2e/upload-pdf-files-demo-video.cy.ts --no-runner-ui --config-file cypress-demo-mobile.config.ts",
    "demo-desktop-video": "cypress run --spec cypress/e2e/upload-pdf-files-demo-video.cy.ts --no-runner-ui --config-file cypress-demo-desktop.config.ts",
    "gif": "gifify -r 10@1 -o public/gifs/demo_mobile_video public/videos/demo_mobile_video.mp4"
  },
  "dependencies": {
    "@heroui/react": "^2.7.5",
    "framer-motion": "^12.6.2",
    "jspdf": "^3.0.1",
    "next": "^15.2.4",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-pdftotext": "^1.3.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^14.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "postcss": "^8",
    "prettier": "3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  }
}
